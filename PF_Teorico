import numpy as np
def PF_Teorico (PRICING, PAYMENTS):	

CANT_PRICING_TYPES = select COUNT (DISTICT PRICING_TYPES_ID) from PRICING
	
	For i = 1 to CANT_PRICING_TYPES
		
		// Me fijo cuales variables están en juego en ese PRICING_TYPE
		VARIABLES = Select (DISTICT FIELD_NAME) from PRICING where PRICING_PRIORIDAD = 1 
    // ejemplos [‘payment_method_id’,’collector_id’]
		
    CANT_VARIABLES = VARIABLES.SHAPE[1]
		
		// ARMO EL SET DE RESTRICCIONES 
		REGLAS = Select id_AUTOINCREMENT, PRICING_RULE_ID from PRICING where PRICING_PRIORIDAD = i
  	CANT_REGLAS = Select (DISTICT PRICING_RULE_ID) from PRICING where PRICING_PRIORIDAD = i 
    DETALLE_REGLA = Select PRICING_RULE_ID, FIELD_NAME, OPERATOR, VALUE_EXPRESSION, FEE from PRICING where PRICING_PRIORIDAD = i

		// ELIJO EL PF_teorico
		payments = 
		SELECT VARIABLES FROM PAYMENTS
		
		INNER JOIN  PRICING ON 
			For j to CANT_REGLAS
				For K to CANT_VARIABLES					
					If OPERATOR = not equals THEN
					PAYMENTS.FIELD_NAME = PRICING.VALUE_EXPRESSION


INNER JOIN ENTRE RESTRICCIONES Y PAYMENTS EN VARIABLES 	
	
	return (payments)	

